{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/view/(.*)",
      "dest": "/public/main.html"
    },
    {
      "src": "/",
      "dest": "/public/main.html"
    }
  ]
}
