{
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/view/(.*)", "dest": "/src/main.html" },
    { "src": "/", "dest": "/src/main.html" },
    { "src": "/assets/(.*)", "dest": "/src/assets/$1" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}